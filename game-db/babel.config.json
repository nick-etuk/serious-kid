{
    "presets": [
        [
            "@babel/preset-env",
            {
            "targets": {
                "edge": "17",
                "firefox": "60",
                "chrome": "67",
                "safari": "11.1"
            },
            "useBuiltIns": "usage",
            "corejs": "3.6.5"
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        [
          "@babel/plugin-transform-runtime",
           {
             "helpers": true,
             "regenerator": true
           }
        ],
        ["@babel/plugin-proposal-private-methods", { "loose": true }]
    ]
}
